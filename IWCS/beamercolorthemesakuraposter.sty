\setbeamercolor{normal text}{fg=sDarkGray}
%\setbeamercolor{titlelike}{use=normal text, parent=normal text}
\setbeamercolor{title in headline}{%
  fg=white
}
\setbeamercolor{author in headline}{%
  fg=white
}
\setbeamercolor{institute in headline}{%
  use=white
}
\setbeamercolor{footline}{%
  fg=white,
  bg=sPurple
}
\setbeamercolor{headline}{%
    fg=white,
    bg=sDarkBlue
}
\setbeamertemplate{headline}{%
    \leavevmode
    \begin{beamercolorbox}[wd=\paperwidth]{headline}
        \centering
        \vskip4ex
        \usebeamercolor{title in headline}{\color{fg}\textbf{\LARGE{\inserttitle}}\\[2.5ex]}
        \usebeamercolor{author in headline}{\color{fg}\large{\insertauthor}\\[1.2ex]}
        \usebeamercolor{institute in headline}{\color{fg}\large{\insertinstitute}}
        \vskip2ex
    \end{beamercolorbox}
}
\setbeamercolor{block title}{%
  fg=white,
  bg=sPurple
}
\setbeamercolor{block body}{%
  fg=sDarkGray,
  bg=sLightBrown
}
\setbeamercolor{block alerted title}{%
  bg=sPink,
  fg=white
}
\setbeamercolor{block alerted body}{%
  fg=sDarkGray,
  bg=sLighterGray
}
\setbeamercolor{itemize item}{%
  fg=sDarkBlue
}
\setbeamercolor{local structure}{%
  fg=sDarkGray
}
\setbeamercolor{footline}{%
  bg=sDarkGray,
  fg=white
}
\setbeamercolor{bibliography entry title}{%
  fg=sDarkGray
}
\setbeamercolor{bibliography entry author}{%
  fg=sDarkGray
}
\setbeamercolor{bibliography entry note}{%
  fg=sDarkGray
}
\mode<all>
